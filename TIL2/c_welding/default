# Default server configuration
#
server {
      
	 listen 443 ssl;
         server_name careers.machint.com;


         ssl_certificate     /home/machint/careers/careers_machint_com.crt;
         ssl_certificate_key /home/machint/careers/careers_machint_com.key;


	 location /{
           autoindex on;
           root /home/machint/careers/html;
         }
}

